{
  "name": "homelab-portfolio",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "yarn lint:js",
    "deploy": "push-dir --dir=dist --branch=gh-pages"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
